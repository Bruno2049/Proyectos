MONEDERO_CONSUMO &gt; 0 and 
PERSONA_ID IN( SELECT PERSONA_ID FROM EC_PERSONAS WHERE  EC_PERSONAS.SUSCRIPCION_ID = EC_SUSCRIPCION.SUSCRIPCION_ID)) AS CONSUMOS, 888888 as FechaI, 999999 as FechaF 
from EC_SUSCRIPCION WHERE SUSCRIPCION_ID &gt; 0 ORDER BY SUSCRIPCION_NOMBRE</CommandText>

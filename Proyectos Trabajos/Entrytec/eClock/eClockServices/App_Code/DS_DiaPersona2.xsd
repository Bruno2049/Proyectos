                         EC_TIPO_INC_SIS.TIPO_INC_SIS_NOMBRE AS INCIDENCIA_SISTEMA, EC_PERSONAS.PERSONA_ID
FROM            EC_SUSCRIPCION INNER JOIN
                         EC_PERSONAS ON EC_SUSCRIPCION.SUSCRIPCION_ID = EC_PERSONAS.SUSCRIPCION_ID INNER JOIN
                         EC_GRUPOS_2 ON EC_PERSONAS.GRUPO_2_ID = EC_GRUPOS_2.GRUPO_2_ID INNER JOIN
                         EC_GRUPOS_3 ON EC_PERSONAS.GRUPO_3_ID = EC_GRUPOS_3.GRUPO_3_ID INNER JOIN
                         EC_PERSONAS_DIARIO ON EC_PERSONAS.PERSONA_ID = EC_PERSONAS_DIARIO.PERSONA_ID INNER JOIN
                         EC_TURNOS_DIA ON EC_PERSONAS_DIARIO.TURNO_DIA_ID = EC_TURNOS_DIA.TURNO_DIA_ID INNER JOIN
                         EC_TURNOS ON EC_PERSONAS.TURNO_ID = EC_TURNOS.TURNO_ID INNER JOIN
                         EC_INCIDENCIAS ON EC_PERSONAS_DIARIO.INCIDENCIA_ID = EC_INCIDENCIAS.INCIDENCIA_ID INNER JOIN
                         EC_TIPO_INCIDENCIAS ON EC_INCIDENCIAS.TIPO_INCIDENCIA_ID = EC_TIPO_INCIDENCIAS.TIPO_INCIDENCIA_ID INNER JOIN
                         EC_TIPO_INC_SIS ON EC_PERSONAS_DIARIO.TIPO_INC_SIS_ID = EC_TIPO_INC_SIS.TIPO_INC_SIS_ID
WHERE        (EC_PERSONAS.PERSONA_LINK_ID = ?) AND (EC_PERSONAS_DIARIO.PERSONA_DIARIO_FECHA = ?) AND 
                         (EC_PERSONAS.PERSONA_BORRADO = 0)</CommandText>

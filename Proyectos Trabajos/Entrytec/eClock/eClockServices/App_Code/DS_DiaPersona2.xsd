                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="False">
                    <CommandText>SELECT DISTINCT 
                         EC_PERSONAS.PERSONA_LINK_ID AS PERSONA_LINK_ID, EC_SUSCRIPCION.SUSCRIPCION_NOMBRE AS CENTRO_COSTOS, 

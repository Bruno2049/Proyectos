FROM            (SELECT        SUSCRIPCION_NOMBRE, GRUPO_2_NOMBRE, PERSONA_NOMBRE, PERSONA_LINK_ID, DIAS_TRABAJO, CASE WHEN HoraEntrada IS NULL 
                                                    THEN 'F' WHEN CONVERT(DATETIME, CONVERT(char, HoraEntrada, 108), 102) &gt;= CONVERT(DATETIME, CONVERT(char, 
                                                    TURNO_DIA_HERETARDO, 108), 102) THEN CONVERT(varchar, HoraEntrada, 108) + ' R' ELSE CONVERT(varchar, HoraEntrada, 108) 
                                                    END AS DiaResultado, TURNO_DIA_HE, TURNO_DIA_HERETARDO, PERSONA_ID, TURNO_DIA_ID
                          FROM            (SELECT        (SELECT        MIN(ACCESO_FECHAHORA) AS Fecha
                                                                               FROM            EC_ACCESOS
                                                                               WHERE        (ACCESO_FECHAHORA &gt;= EC_DIAS_TRABAJO.DIAS_TRABAJO) AND (ACCESO_FECHAHORA &lt; DATEADD(DW, 1, 
                                                                                                        EC_DIAS_TRABAJO.DIAS_TRABAJO)) AND (TIPO_ACCESO_ID = 2)
                                                                               GROUP BY PERSONA_ID
                                                                               HAVING         (PERSONA_ID = EC_PERSONAS.PERSONA_ID)) AS HoraEntrada, EC_DIAS_TRABAJO.DIAS_TRABAJO, 
                                                                              EC_PERSONAS.PERSONA_NOMBRE, EC_GRUPOS_2.GRUPO_2_NOMBRE, EC_SUSCRIPCION.SUSCRIPCION_NOMBRE, 
                                                                              EC_PERSONAS.PERSONA_LINK_ID, EC_TURNOS_DIA.TURNO_DIA_HERETARDO, EC_PERSONAS.PERSONA_ID, 
                                                                              EC_TURNOS_DIA.TURNO_DIA_HE, EC_TURNOS_DIA.TURNO_DIA_ID, EC_SUSCRIPCION.SUSCRIPCION_ID
                                                    FROM            EC_PERSONAS INNER JOIN
                                                                              EC_PERSONAS_DIARIO ON EC_PERSONAS.PERSONA_ID = EC_PERSONAS_DIARIO.PERSONA_ID INNER JOIN
                                                                              EC_DIAS_TRABAJO ON EC_PERSONAS_DIARIO.PERSONA_DIARIO_FECHA = EC_DIAS_TRABAJO.DIAS_TRABAJO INNER JOIN
                                                                              EC_SUSCRIPCION ON EC_PERSONAS.SUSCRIPCION_ID = EC_SUSCRIPCION.SUSCRIPCION_ID INNER JOIN
                                                                              EC_GRUPOS_2 ON EC_PERSONAS.GRUPO_2_ID = EC_GRUPOS_2.GRUPO_2_ID INNER JOIN
                                                                              EC_TURNOS_DIA ON EC_PERSONAS_DIARIO.TURNO_DIA_ID = EC_TURNOS_DIA.TURNO_DIA_ID
                                                    WHERE        (EC_DIAS_TRABAJO.DIAS_TRABAJO &gt;= ?) AND (EC_DIAS_TRABAJO.DIAS_TRABAJO &lt;= ?) AND (EC_SUSCRIPCION.SUSCRIPCION_ID = ?)) 
                                                    AS Tabla) AS TablaFinal
ORDER BY SUSCRIPCION_NOMBRE, GRUPO_2_NOMBRE, PERSONA_NOMBRE, DIAS_TRABAJO</CommandText>

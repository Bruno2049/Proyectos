<UserControl x:Class="eClock5.Controles.UC_RangoFechas"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="22" d:DesignWidth="300" IsVisibleChanged="UserControl_IsVisibleChanged">
    <StackPanel Orientation="Horizontal">
        <TextBlock x:Name="Lbl_Periodo" TextWrapping="Wrap" Text="Periodo" Foreground="{DynamicResource Blanco_Brush}" FontFamily="{DynamicResource EntryTecFont}" FontSize="{DynamicResource FontSizeSubTitulo}"  Height="22" Margin="10,0,10,0"/>
        <DatePicker  x:Name="Wdp_Inicio" SelectedDateChanged="Wdp_Inicio_SelectedDateChanged" />
        <DatePicker  x:Name="Wdp_Fin"  Margin="10,0,10,0" SelectedDateChanged="Wdp_Fin_SelectedDateChanged"/>
    </StackPanel>
</UserControl>

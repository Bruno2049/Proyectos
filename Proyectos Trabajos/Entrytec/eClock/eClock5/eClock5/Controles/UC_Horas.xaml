<UserControl x:Class="eClock5.Controles.UC_Horas"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" Height="23" Width="56" Background="{DynamicResource Blanco_Brush}" >
    <Grid>
        <TextBox x:Name="Tbx_Horas" TextWrapping="Wrap" Background="{x:Null}" TextChanged="Tbx_Horas_TextChanged" Text="00:00" />

    </Grid>
</UserControl>

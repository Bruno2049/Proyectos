<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:eClock5="clr-namespace:eClock5" 
             xmlns:Actividades="clr-namespace:eClock5.Vista.Actividades" 
             xmlns:local="clr-namespace:eClock5.Vista.Tramites" 
             xmlns:Modelos="clr-namespace:eClockBase.Modelos;assembly=eClockBase" 
             x:Class="eClock5.Vista.Tramites.Listado" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>

        <eClock5:UC_Listado Height="Auto" Width="Auto" Modelos="eClockBase.Modelos.Tramites.Reporte_NoTramites, eClockBase.Modelos.Tramites.Reporte_Detalle, eClockBase.Modelos.Solicitudes.Model_ReporteSolicitudVacaciones" CampoLlave="TRAMITE_ID" CampoNombre="TRAMITE_FECHA" CampoDescripcion="TRAMITE_DESCRIPCION" Tabla="EC_TRAMITES" >
            <eClock5:UC_Listado.ControlEdicion>
                <local:EdicionTramites/>
            </eClock5:UC_Listado.ControlEdicion>
            <eClock5:UC_Listado.DataContext>
                <Modelos:Model_TRAMITES/>
            </eClock5:UC_Listado.DataContext>
        </eClock5:UC_Listado>

    </Grid>
</UserControl>

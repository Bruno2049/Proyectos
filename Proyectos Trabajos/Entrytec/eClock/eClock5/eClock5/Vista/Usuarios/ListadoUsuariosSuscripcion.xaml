        <eClock5:UC_Listado Width="Auto" Height="Auto" x:Name="Lst_Usuarios" Tabla="EC_USUARIOS" CampoDescripcion="USUARIO_NOMBRE" CampoNombre="USUARIO_USUARIO" CampoLlave="USUARIO_ID" Filtro="SUSCRIPCION_ID = @SUSCRIPCION_ID" >            

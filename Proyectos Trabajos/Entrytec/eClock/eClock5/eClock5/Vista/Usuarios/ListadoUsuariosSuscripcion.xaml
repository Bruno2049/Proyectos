             xmlns:Modelos="clr-namespace:eClockBase.Modelos;assembly=eClockBase" 
             x:Class="eClock5.Vista.Usuarios.ListadoUsuariosSuscripcion"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">

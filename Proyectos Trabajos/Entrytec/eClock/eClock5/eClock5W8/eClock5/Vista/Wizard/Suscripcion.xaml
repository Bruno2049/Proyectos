<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:Suscripciones="clr-namespace:eClock5.Vista.Suscripciones" x:Class="eClock5.Vista.Wizard.Suscripcion" 
             mc:Ignorable="d" 
             d:DesignHeight="375" d:DesignWidth="570">
    <Grid>
        <Image HorizontalAlignment="Left" Height="{StaticResource AltoWizardTitulo}" VerticalAlignment="Top" Width="100" Source="/eClock5;component/Resources/eClock.png"/>
        <TextBlock x:Name="Lbl_Suscripcion" TextWrapping="Wrap" Text="Suscripción" VerticalAlignment="Top" FontSize="{StaticResource FontSizeTitulo}" Margin="200,10,200,0" TextAlignment="Center" Foreground="{StaticResource Blanco_Brush}"/>
        <Suscripciones:EdicionSuscripcionCliente Margin="0,69,0,0"/>


    </Grid>
</UserControl>

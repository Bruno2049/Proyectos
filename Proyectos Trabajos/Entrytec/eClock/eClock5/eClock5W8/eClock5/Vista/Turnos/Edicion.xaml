            <TextBlock x:Name="Lbl_TURNO_COLOR" Text="Color: " Grid.Column="0" Grid.Row="3" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="{DynamicResource Blanco_Brush}" Height="25" Margin="10,0,0,0" Width="41" />

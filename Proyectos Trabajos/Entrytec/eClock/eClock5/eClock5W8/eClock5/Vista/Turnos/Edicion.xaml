                <RowDefinition Height="25" x:Name="R_Bloque"/>
                <RowDefinition Height="25" x:Name="R_Tolerancia"/>
                <RowDefinition Height="25" x:Name="R_Tolerancia2"/>
                <RowDefinition Height="25" x:Name="R_Tolerancia3"/>
                <RowDefinition Height="25" x:Name="Tolerancia4"/>
                <RowDefinition Height="25" x:Name="R_ToleranciaComida"/>

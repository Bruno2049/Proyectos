            <Controles:UC_DateTimePicker x:Name="DT_TURNO_DIA_HERETARDO" Grid.Row="8" HorizontalAlignment="Left" HorizontalContentAlignment="Center" VerticalAlignment="Center"  Margin="209,7,0,6" Checked="True" ShowDropDown="False" ShowCheckBox="False" Format="Custom" CustomFormat="HH:mm"  Width="Auto" />
            <TextBlock x:Name="Lbl_TURNO_DIA_HERETARDO_B" Text="Minutos para Retardo Mínimo" Grid.Column="0" Grid.Row="9" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="{DynamicResource Blanco_Brush}" Margin="10,0,0,0" Width="Auto" Height="Auto" />

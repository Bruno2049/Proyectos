            <TextBlock x:Name="Lbl_TURNO_DIA_HERETARDO_C" Text="Minutos para Retardo Máximo" Grid.Column="0" Grid.Row="10" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="{DynamicResource Blanco_Brush}" Margin="10,0,0,0"  Width="Auto" Height="Auto" />
            <Controles:UC_DateTimePicker x:Name="DT_TURNO_DIA_HERETARDO_C" Grid.Row="10" HorizontalAlignment="Left" HorizontalContentAlignment="Center" VerticalAlignment="Center"  Margin="209,7,0,6" Checked="True" ShowDropDown="False" ShowCheckBox="False" Format="Custom" CustomFormat="HH:mm"  Width="Auto" />
            <TextBlock x:Name="Lbl_TURNO_DIA_HERETARDO_D" Text="Minutos para Retardo Superior" Grid.Column="0" Grid.Row="11" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="{DynamicResource Blanco_Brush}" Margin="10,0,0,0"  Width="Auto" Height="Auto" />
            <Controles:UC_DateTimePicker x:Name="DT_TURNO_DIA_HERETARDO_D" Grid.Row="11" HorizontalContentAlignment="Center" VerticalAlignment="Center"  Margin="210,7,0,6" Checked="True" ShowDropDown="False" ShowCheckBox="False" Format="Custom" CustomFormat="HH:mm"  Width="33" HorizontalAlignment="Left" />

<Window x:Class="ReproductorMusica.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" WindowStartupLocation="CenterScreen" Background="#FFE8E8E8">
    <Grid>
        <WrapPanel Orientation="Horizontal">

            <Ellipse Width="100" Height="100" Fill="Gold" />
            <Ellipse Width="100" Height="100" Fill="Tomato"/>
            <Ellipse Width="100" Height="100" Fill="LightBlue" Margin="-50,50" />
            <Ellipse Width="100" Height="100" Fill="YellowGreen" />

        </WrapPanel>
    </Grid>
</Window>

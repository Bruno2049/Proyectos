<UserControl x:Class="PresentacionFacturas.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="800" d:DesignWidth="700">

    <Grid x:Name="LayoutRoot" Background="White">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="81*"/>
            <ColumnDefinition Width="70*"/>
            <ColumnDefinition Width="199*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="3*"/>
            <RowDefinition Height="37*"/>
        </Grid.RowDefinitions>
        <Button Content="Insertar" HorizontalAlignment="Left" Height="29" Margin="31,0,0,21" VerticalAlignment="Bottom" Width="96" Click="Button_Click"/>
        <Button Content="Listar" HorizontalAlignment="Left" Height="29" Margin="13,10,0,0" VerticalAlignment="Top" Width="96" Grid.Column="1" Click="Button_Click_1"/>
        <Canvas x:Name="CanvasPrincipalv" HorizontalAlignment="Left" Height="740" VerticalAlignment="Top" Width="700" Grid.Row="1" Grid.ColumnSpan="3"/>

    </Grid>
</UserControl>
